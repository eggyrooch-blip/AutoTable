    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.4.0",
    "vite": "^5.4.10"
  },
  "dependencies": {
    "@douyinfe/semi-foundation": "^2.38.0",
    "@douyinfe/semi-ui": "^2.36.0",
    "@lark-base-open/js-sdk": "^0.5.0",
